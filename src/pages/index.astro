---
import HeadingDog from "@/components/HeadingDog.astro";
import Welcome from "../components/Welcome.astro";
import Layout from "../layouts/Layout.astro";

const imgDog = [
	{
		src: "/img/dogs/dog.webp",
		top: "10%",
		left: "10%",
		rotate: "0deg",
		scale: 1.1,
	},
	{
		src: "/img/dogs/dog-1.webp",
		top: "90%",
		left: "10%",
		rotate: "0deg",
		scale: 0.9,
	},
	{
		src: "/img/dogs/dog-2.webp",
		top: "20%",
		left: "60%",
		rotate: "-5deg",
		scale: 1,
	},
	{
		src: "/img/dogs/dog-3.webp",
		top: "50%",
		left: "0%",
		rotate: "-5deg",
		scale: 1.3,
	},
	{
		src: "/img/dogs/dog-4.webp",
		top: "10%",
		left: "90%",
		rotate: "-5deg",
		scale: 1.3,
	},
	{
		src: "/img/dogs/dog-5.webp",
		top: "60%",
		left: "70%",
		rotate: "-5deg",
		scale: 1.3,
	},
	{
		src: "/img/dogs/dog-6.webp",
		top: "80%",
		left: "50%",
		rotate: "-5deg",
		scale: 1.3,
	},
];
---

<Layout>
	<HeadingDog img={imgDog} />
  <Welcome />
</Layout>